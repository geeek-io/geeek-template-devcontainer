{
	"[dockercompose]": {
		"editor.defaultFormatter": "redhat.vscode-yaml"
	},
	"chat.agent.enabled": true,
	"chat.commandCenter.enabled": true,
	"chat.detectParticipant.enabled": true,
	"chat.fontSize": 16,
	"chat.renderRelatedFiles": true,
	"containers.contexts.showInStatusBar": true,
	"docker.lsp.telemetry": "off",
	"editorconfig.generateAuto": false,
	"editorconfig.showMenuEntry": false,
	"evenBetterToml.schema.links": true,
	"evenBetterToml.semanticTokens": true,
	"evenBetterToml.taplo.bundled": false,
	"files.associations": {
		"*.env": "properties"
	},
	"geminicodeassist.agentYoloMode": true,
	"geminicodeassist.enableTelemetry": false,
	"geminicodeassist.inlineSuggestions.suggestionSpeed": "Slow",
	"github.copilot.chat.scopeSelection": true,
	"github.copilot.nextEditSuggestions.enabled": true,
	"shellcheck.run": "onSave",
	"shfmt.executablePath": "./scripts/shfmt.sh",
	"yaml.disableDefaultProperties": true,
	"yaml.format.singleQuote": true,
	"yaml.schemas": {
		"https://raw.githubusercontent.com/docker/vscode-extension/6a88caada42b57090df7ce91ec2a6561b422afe1/misc/empty.json": [
			"compose*y*ml",
			"docker-compose*y*ml"
		]
	}
}
