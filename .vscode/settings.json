{
	"[dockercompose]": {
		"editor.defaultFormatter": "redhat.vscode-yaml"
	},
	"chat.agent.enabled": true,
	"chat.agentSessionsViewLocation": "view",
	"chat.commandCenter.enabled": true,
	"chat.detectParticipant.enabled": true,
	"chat.fontSize": 16,
	"chat.math.enabled": true,
	"chat.mcp.assisted.nuget.enabled": true,
	"chat.mcp.autostart": "newAndOutdated",
	"chat.renderRelatedFiles": true,
	"chat.todoListTool.enabled": true,
	"containers.contexts.showInStatusBar": true,
	"docker.lsp.experimental.scout.notPinnedDigest": true,
	"docker.lsp.experimental.scout.recommendedTag": true,
	"docker.lsp.telemetry": "off",
	"editorconfig.generateAuto": false,
	"editorconfig.showMenuEntry": false,
	"evenBetterToml.schema.links": true,
	"evenBetterToml.semanticTokens": true,
	"evenBetterToml.taplo.bundled": false,
	"files.associations": {
		"*.env": "shellscript"
	},
	"geminicodeassist.agentYoloMode": true,
	"geminicodeassist.enableTelemetry": false,
	"geminicodeassist.inlineSuggestions.nextEditPredictions": true,
	"geminicodeassist.inlineSuggestions.suggestionSpeed": "Slow",
	"github.copilot.chat.agent.thinkingTool": true,
	"github.copilot.chat.alternateGptPrompt.enabled": true,
	"github.copilot.chat.codesearch.enabled": true,
	"github.copilot.chat.commitMessageGeneration.instructions": [
		{
			"text": "Use conventional commit message format."
		}
	],
	"github.copilot.chat.generateTests.codeLens": true,
	"github.copilot.chat.languageContext.fix.typescript.enabled": true,
	"github.copilot.chat.languageContext.inline.typescript.enabled": true,
	"github.copilot.chat.languageContext.typescript.enabled": true,
	"github.copilot.chat.newWorkspace.useContext7": true,
	"github.copilot.chat.notebook.enhancedNextEditSuggestions.enabled": true,
	"github.copilot.chat.notebook.followCellExecution.enabled": true,
	"github.copilot.chat.scopeSelection": true,
	"github.copilot.chat.useResponsesApi": true,
	"github.copilot.nextEditSuggestions.enabled": true,
	"inlineChat.enableV2": true,
	"inlineChat.lineEmptyHint": true,
	"inlineChat.notebookAgent": true,
	"shellcheck.run": "onSave",
	"yaml.disableDefaultProperties": true,
	"yaml.format.proseWrap": "always",
	"yaml.format.singleQuote": true,
	"yaml.schemas": {
		"https://raw.githubusercontent.com/docker/vscode-extension/6a88caada42b57090df7ce91ec2a6561b422afe1/misc/empty.json": [
			"compose*y*ml",
			"docker-compose*y*ml"
		]
	},
	"yaml.style.flowMapping": "forbid",
	"yaml.style.flowSequence": "forbid"
}
