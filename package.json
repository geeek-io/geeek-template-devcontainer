{
	"$schema": "https://www.schemastore.org/package.json",
	"devDependencies": {
		"@upstash/context7-mcp": "1.0.21",
		"prettier": "3.6.2"
	},
	"devEngines": {
		"os": {
			"name": "linux",
			"onFail": "error"
		},
		"packageManager": [
			{
				"name": "pnpm",
				"onFail": "error",
				"version": "10.18.1"
			}
		],
		"runtime": [
			{
				"name": "node",
				"onFail": "error",
				"version": "24.9.0"
			},
			{
				"name": "bun",
				"onFail": "download",
				"version": "1.2.23"
			}
		]
	},
	"engineStrict": true,
	"license": "Apache-2.0",
	"name": "@repo/root",
	"private": true,
	"type": "module",
	"workspaces": [
		"workspaces/*/*"
	]
}
